{
  "name": "microcade-server",
  "dependencies": {
    "adjective-adjective-animal": "^1.3.3",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "react": ">=15",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "socket.io": "^2.3.0",
    "socket.io-client": "^2.3.0",
    "styled-components": "^5.1.1",
    "typescript": "^3.9.7",
