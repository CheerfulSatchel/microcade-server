    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/socket.io": "^2.1.10",
    "@types/socket.io-client": "^1.4.33",
    "@types/styled-components": "^5.1.1",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
